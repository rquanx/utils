{
  "name": "@rquanx/utils",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.4.0",
  "description": "Opinionated collection of common JavaScript / TypeScript utils by @rquanx",
  "author": "rquanx",
  "license": "MIT",
  "funding": "https://github.com/sponsors/rquanx",
  "homepage": "https://github.com/rquanx/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rquanx/utils.git"
  },
  "bugs": {
    "url": "https://github.com/rquanx/utils/issues"
  },
  "keywords": [
    "utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint-fix": "pnpm lint --fix",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp && pnpm publish",
    "start": "tsx src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.67.2",
    "crypto-js": "^4.2.0",
    "nanoid": "^5.1.3",
    "node-wmic": "^2.0.0",
    "pathe": "^2.0.3",
    "playwright": "^1.51.0",
    "systeminformation": "^5.25.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.2.0",
    "@types/node": "^22.13.4",
    "bumpp": "^10.0.3",
    "eslint": "^9.20.1",
    "tsx": "^4.8.0",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vite": "^6.1.0",
    "vitest": "^3.0.5"
  }
}
